{{#each all}}
    <div class="famousList">
        <h3>{{name}}</h3>
        <h4>{{occupation}}</h4>
        <p>{{catchPhrase}}</p>
    </div>
{{/each}}


<style>
    .famousList {
        display: grid;
        grid-template-rows: repeat(auto-fill, 2fr);
        place-items: center;
        margin-bottom: 30px;
    } 
